<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.favorite.model.FavoriteVO" table="Favorite">

		<composite-id>
			<key-property name="MemberAcc" column="MemberAcc" />
			<key-property name="RestID" column="RestID" />
		</composite-id>
		
		 <many-to-one name="RestaurantVO" class="com.restaurant.model.RestaurantVO" lazy="false" insert="false" update="false">
			<column name="RestID" />
		</many-to-one>

	</class>
</hibernate-mapping>