<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.servicearea.model.ServiceAreaVO" table="ServiceArea" >
        
        <id name="ServiceAreaID" column="ServiceAreaID">
            <generator class="identity"/>
        </id>
        
        <property name="City" column="City" />
        <property name="Area" column="Area" />
        <property name="RestID" column="RestID"/>
<!--         <property name="RestID" column="RestID" /> -->
       <many-to-one name="RestaurantVO" class="com.restaurant.model.RestaurantVO" lazy="false" insert="false" update="false">
            <column name="RestID" />
        </many-to-one> 
        
    </class>
</hibernate-mapping>

