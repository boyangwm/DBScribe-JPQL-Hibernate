<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.ad.model.AdVO" table="Advertisement" >
        
        <id name="RestID" column="RestID"></id>
        <property name="TreatID" column="TreatID" />
        <property name="Context" column="Context" />
        <property name="Image" column="Image" />
        <property name="StartDate" column="StartDate" />
        <property name="EndDate" column="EndDate" />
        <property name="TreatDetail" column="TreatDetail" />
        <many-to-one name="TreatCaseVO" class="com.treatcase.model.TreatCaseVO" lazy="false" insert="false" update="false">
			<column name="TreatID" />
		</many-to-one>
        <many-to-one name="RestaurantVO" class="com.restaurant.model.RestaurantVO" lazy="false" insert="false" update="false">
			<column name="RestID" />
		</many-to-one>
    </class>
</hibernate-mapping>

